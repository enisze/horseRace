{
  "name": "@acme/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "npx expo start --ios",
    "dev:android": "npx expo start --android",
    "dev:ios": "npx expo start --ios",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "android": "expo run:android",
    "ios": "npx expo run:ios",
    "build:dev-client:ios-simulator": "eas build --platform ios --profile preview --local",
    "build:dev-client:android": "export ANDROID_HOME=$HOME/Library/Android/sdk/ && eas build --platform android --profile development --local",
    "start-dev-client": "npx expo start --dev-client",
    "export": "expo export --platform android "
  },
  "dependencies": {
    "@expo/metro-config": "^0.18.3",
    "@shopify/flash-list": "1.6.4",
    "@tanstack/react-query": "^4.33.0",
    "@trpc/client": "^10.38.1",
    "@trpc/react-query": "^10.38.1",
    "@trpc/server": "^10.38.1",
    "expo": "^51.0.9",
    "expo-build-properties": "^0.12.1",
    "expo-constants": "~16.0.2",
    "expo-linking": "~6.3.1",
    "expo-localization": "^15.0.3",
    "expo-router": "3.5.14",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "^0.25.15",
    "i18next": "^23.4.6",
    "jotai": "^2.4.1",
    "metro": "0.80.9",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.2.1",
    "react-native": "0.74.1",
    "react-native-dotenv": "^3.4.9",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-google-mobile-ads": "^12.1.0",
    "react-native-paper": "^5.10.4",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-vector-icons": "^10.0.0",
    "superjson": "1.13.1"
  },
  "devDependencies": {
    "@acme/api": "^0.1.0",
    "@acme/eslint-config": "^0.2.0",
    "@acme/prettier-config": "^0.1.0",
    "@acme/tailwind-config": "^0.1.0",
    "@acme/tsconfig": "^0.1.0",
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.22.14",
    "@babel/runtime": "^7.22.11",
    "@expo/config-plugins": "^8.0.4",
    "@types/babel__core": "^7.20.1",
    "@types/react": "^18.2.79",
    "eslint": "^8.48.0",
    "expo-dev-client": "^4.0.15",
    "prettier": "^3.0.3",
    "tailwindcss": "3.3.2",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/react"
    ]
  },
  "prettier": "@acme/prettier-config"
}
