{
  "name": "@acme/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "npx expo start --ios",
    "dev:android": "npx expo start --android",
    "dev:ios": "npx expo start --ios",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "android": "expo run:android",
    "ios": "npx expo run:ios",
    "dev-client:ios-simulator": "eas build --platform ios --profile preview --local",
    "dev-client:android": "export ANDROID_HOME=$HOME/Library/Android/sdk/ && eas build --platform android --profile development --local",
    "start-dev-client": "npx expo start --dev-client",
    "export": "expo export --p android"
  },
  "dependencies": {
    "@expo/metro-config": "^0.10.7",
    "@shopify/flash-list": "1.4.3",
    "@tanstack/react-query": "^4.33.0",
    "@trpc/client": "^10.38.0",
    "@trpc/react-query": "^10.38.1",
    "@trpc/server": "^10.38.0",
    "expo": "^49.0.8",
    "expo-build-properties": "^0.8.3",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-localization": "^14.3.0",
    "expo-router": "2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "i18next": "^23.4.6",
    "jotai": "^2.4.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.2.1",
    "react-native": "0.72.4",
    "react-native-dotenv": "^3.4.9",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-google-mobile-ads": "^12.1.0",
    "react-native-paper": "^5.10.4",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1",
    "superjson": "1.13.1"
  },
  "devDependencies": {
    "@acme/api": "^0.1.0",
    "@acme/eslint-config": "^0.2.0",
    "@acme/prettier-config": "^0.1.0",
    "@acme/tailwind-config": "^0.1.0",
    "@acme/tsconfig": "^0.1.0",
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.14",
    "@babel/runtime": "^7.22.11",
    "@expo/config-plugins": "^7.2.5",
    "@types/babel__core": "^7.20.1",
    "@types/react": "^18.2.21",
    "eslint": "^8.48.0",
    "expo-dev-client": "^2.4.8",
    "prettier": "^3.0.3",
    "tailwindcss": "3.3.2",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/react"
    ]
  },
  "prettier": "@acme/prettier-config"
}
