{
  "name": "horserace",
  "version": "0.1.0",
  "private": true,
  "main": "index.tsx",
  "engines": {
    "node": ">=v18.8.0"
  },
  "packageManager": "pnpm@8.7.0",
  "scripts": {
    "clean": "git clean -xdf node_modules",
    "dev": "npx expo start --android",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "android": "expo run:android",
    "ios": "npx expo run:ios",
    "build:dev-client:ios-simulator": "eas build --platform ios --profile preview --local",
    "build:dev-client:android": "export ANDROID_HOME=$HOME/Library/Android/sdk/ && eas build --platform android --profile development --local",
    "start-dev-client": "npx expo start --dev-client",
    "export": "expo export --platform android",
    "db:push": "pnpm -F db push",
    "db:studio": "pnpm -F db dev",
    "format:fix": "prettier --w \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "lint:fix": "todo"
  },
  "dependencies": {
    "@expo/metro-config": "^0.10.7",
    "@shopify/flash-list": "1.4.3",
    "@tanstack/react-query": "^4.33.0",
    "@trpc/client": "^10.38.0",
    "@trpc/react-query": "^10.38.1",
    "@trpc/server": "^10.38.0",
    "autoprefixer": "^10.4.15",
    "expo": "^49.0.8",
    "expo-build-properties": "^0.8.3",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-localization": "^14.3.0",
    "expo-router": "2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "i18next": "^23.4.6",
    "jotai": "^2.4.1",
    "nativewind": "^2.0.11",
    "postcss": "^8.4.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.2.1",
    "react-native": "0.72.4",
    "react-native-dotenv": "^3.4.9",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-google-mobile-ads": "^12.1.0",
    "react-native-paper": "^5.10.4",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1",
    "superjson": "1.13.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.14",
    "@babel/runtime": "^7.22.11",
    "@expo/config-plugins": "^7.2.5",
    "@types/babel__core": "^7.20.1",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "expo-dev-client": "^2.4.8",
    "prettier": "^3.0.3",
    "tailwindcss": "3.3.2",
    "typescript": "^5.2.2"
  }
}
